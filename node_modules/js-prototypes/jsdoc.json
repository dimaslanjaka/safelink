{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/0dd07e7c78d3c4669f4bd9057f721f8bd80eeda9/src/schemas/json/jsdoc-1.0.0.json",
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category",
    "node_modules/better-docs/typedef-import"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_|node_modules",
    "include": [
      "src",
      "README.md"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "better-docs": {
      "name": "Native Javascript Prototype Extender (helper)",
      "logo": "/images/logo.png",
      "title": "js-prototypes",
      "trackingCode": "<!--tracking code-->",
      "hideGenerator": false,
      "component": {
        "entry": [
          "import 'babel-polyfill';",
          "import 'bulma/css/bulma.css';"
        ]
      },
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/dimaslanjaka/js-prototypes"
        },
        {
          "label": "Blog",
          "href": "https://www.webmanajemen.com"
        }
      ]
    }
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./docs/",
    "encoding": "utf8",
    "readme": "README.md",
    "recurse": true
  }
}